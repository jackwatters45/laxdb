    "fumadocs-core": "16.4.1",
    "fumadocs-mdx": "14.2.3",
    "fumadocs-ui": "16.4.1",
    "lucide-static": "^0.561.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "vite": "^7.3.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/mdx": "^2.0.13",
