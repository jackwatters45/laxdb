{
  "name": "@laxdb/pipeline",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "test": "vitest",
    "test:unit": "vitest run --exclude='**/*.integration.test.ts'",
    "test:integration": "vitest run integration",
    "test:coverage": "vitest run --coverage",
    "test:pll": "vitest run src/pll/",
    "test:nll": "vitest run src/nll/",
    "test:mll": "vitest run src/mll/",
    "test:msl": "vitest run src/msl/",
    "test:wla": "vitest run src/wla/",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware --fix",
    "format": "oxfmt",
    "fix": "bun run lint && bun run format"
  },
  "dependencies": {
    "@effect/cli": "^0.73.0",
    "@effect/platform": "^0.94.1",
    "@effect/platform-bun": "^0.87.0",
    "@effect/sql": "^0.49.0",
    "@effect/sql-drizzle": "^0.48.0",
    "@effect/sql-pg": "^0.50.0",
    "@laxdb/core": "*",
    "cheerio": "^1.1.2",
    "drizzle-orm": "^0.44.7",
    "effect": "^3.19.13"
  },
  "devDependencies": {},
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
