{
  "name": "@laxdb/pipeline",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware --fix",
    "format": "oxfmt",
    "fix": "bun run lint && bun run format"
  },
  "dependencies": {
    "@laxdb/core": "*",
    "cheerio": "^1.1.2"
  },
  "devDependencies": {},
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
