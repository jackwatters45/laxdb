{
  "name": "@laxdb/pipeline",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware --fix",
    "format": "oxfmt",
    "fix": "bun run lint && bun run format"
  },
  "dependencies": {
    "@effect/platform": "^0.94.0",
    "cheerio": "^1.1.2",
    "effect": "^3.19.13"
  },
  "devDependencies": {
    "@effect/build-utils": "^0.7.9",
    "@effect/vitest": "latest",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.8",
    "fetchdts": "^0.1.7",
    "tsx": "^4.21.0"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
