    "alchemy": "latest",
    "lefthook": "^2.0.12",
    "miniflare": "^4.20251217.0",
    "oxfmt": "^0.19.0",
    "oxlint": "^1.34.0",
    "oxlint-tsgolint": "^0.10.0",
