{
  "name": "laxdb",
  "version": "0.0.0",
  "private": true,
  "description": "Lax DB is a suite of tools made with lacrosse in mind",
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "deploy": "alchemy deploy",
